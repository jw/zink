# /etc/cron.d/processes: crontab entries for the django application

SHELL=/bin/sh
PATH=/usr/local/sbin:/usr/local/bin:/sbin:/bin:/usr/sbin:/usr/bin

*/10 * * * *  root  /usr/bin/python /var/www/elevenbits/bin/processes.py --properties /var/www/elevenbits/bin/processes.properties >> /tmp/processes.log 2>&1
