[formatters]
keys: detailed,simple
 
[handlers]
#keys: console,file
keys: file
 
[loggers]
keys: root,statistics
 
[formatter_simple]
format: [deployer] [%(asctime)s] [%(levelname)-8s] %(message)s
 
[formatter_detailed]
format: [deployer] [%(asctime)s] [%(levelname)-8s] [%(module)s] %(lineno)d] %(message)s
 
#[handler_console]
#class: StreamHandler
#args: []
#formatter: simple

[handler_file]
class: FileHandler
level: DEBUG
formatter: simple
args: ("./deployer.log", "a")
 
[logger_root]
level: DEBUG
qualname: authserver
handlers: file
#handlers: console, file

[logger_statistics]
level: DEBUG
qualname: statistics
handlers: file
